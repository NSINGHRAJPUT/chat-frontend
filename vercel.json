{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/**",
      "use": "@vercel/node"
    },
    {
      "src": ".gitignore",
      "use": "@vercel/static"
    },
    {
      "src": "package-lock.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/api/(.*)",
      "dest": "http://13.239.27.154:8080/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
